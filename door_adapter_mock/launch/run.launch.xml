<?xml version='1.0' ?>

<launch>

  <arg name="config_file" description="The config file that provides important parameters for setting up the adapter"/>
  <arg name="output" default="screen"/>

  <!-- Mock Door Server -->
  <node pkg="door_adapter_mock"
        exec="mock_door_server"
        output="both">
  </node>

  <!-- Door adapter -->
   <node pkg="door_adapter_mock"
         exec="door_adapter_mock_node"
         args="-c $(var config_file)"
         output="both">
   </node>

</launch>